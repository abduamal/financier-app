<%= render "accounts/blank" %>
<ul id = "accounts">
   <% @accounts.each do |a| %>
    <li>
        <%= link_to a.account_number, {:action => 'show', :id => a.id} -%>
        <b> <%= link_to 'Edit', {:action => "edit", :id => a.id} %></b>
        <b> <%= link_to "Add a new transaction", new_account_transaction_path(a) %>
    </li>
   <% end %>
</ul>

<p><%= link_to "Add a new account", {:action => 'new' }%></p>
<br />

